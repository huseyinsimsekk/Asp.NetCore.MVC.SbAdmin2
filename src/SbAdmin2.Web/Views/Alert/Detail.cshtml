@model Alert
@{
    ViewData["Title"] = "Alert Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">@Model.Title</h6>
    </div>
    <div class="card-body">
        <p>@Model.Description</p> 
    </div>
</div>
